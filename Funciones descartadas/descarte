#Funcion que envia el precio actual del bitcoin
def Bit(update, context):
    bot=context.bot
    chatId=update.message.chat_id
    format_result=report()
    bot.sendMessage(
        chat_id= chatId,
        parse_mode="HTML",
        text=f"{format_result}"
    )


#Funcion que envia datos de una ciudad
def datosCiudad(update, context):
    bot=context.bot
    chatId=update.message.chat_id
    args = context.args
    city=" ".join(args)
    data=ciudad(city)
    
    if data[6] == True:
        bot.sendMessage(
            chat_id= chatId,
            parse_mode="HTML",
            text=f"Pais: {data[3]}
provincia: {data[2]}
latitud: {data[1]}
longitud: {data[0]}
temperatura: {data[4]}
sensaci√≥n termica: {data[5]}"
        )
    else:
        bot.sendMessage(
            chat_id= chatId,
            parse_mode="HTML",
            text=f"{data}"
            )

